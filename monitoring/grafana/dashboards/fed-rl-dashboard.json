{
  "dashboard": {
    "id": null,
    "title": "Dynamic Graph Fed-RL Monitoring",
    "tags": ["federated-learning", "reinforcement-learning", "graphs"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Training Progress",
        "type": "stat",
        "targets": [{
          "expr": "sum(training_episodes_total)",
          "legendFormat": "Total Episodes"
        }],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Agent Performance",
        "type": "timeseries",
        "targets": [{
          "expr": "avg_over_time(episode_reward[5m])",
          "legendFormat": "Avg Reward"
        }],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Federation Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(parameter_exchanges_total[5m])",
            "legendFormat": "Parameter Exchanges/sec"
          },
          {
            "expr": "rate(gossip_messages_total[5m])",
            "legendFormat": "Gossip Messages/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Graph Topology Changes",
        "type": "timeseries",
        "targets": [{
          "expr": "sum(topology_changes_total) by (environment)",
          "legendFormat": "{{environment}}"
        }],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memory Usage"
          },
          {
            "expr": "rate(process_cpu_seconds_total[5m])",
            "legendFormat": "CPU Usage"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "bytes"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      }
    ]
  }
}